<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pri.demo.springboot.mapper.SysSchedulerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="pri.demo.springboot.entity.SysSchedulerEntity">
        <id column="id" property="id"/>
        <result column="task_key" property="taskKey"/>
        <result column="cron" property="cron"/>
        <result column="is_init_start" property="initStart"/>
        <result column="description" property="description"/>
        <result column="is_deleted" property="deleted"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, task_key, cron, is_init_start, `description`, is_deleted, create_time, update_time
    </sql>

</mapper>
